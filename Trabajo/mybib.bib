title = {{Stellarator and tokamak plasmas: a comparison}},
url = {https://doi.org/10.1088{\%}2F0741-3335{\%}2F54{\%}2F12{\%}2F124009 https://iopscience.iop.org/article/10.1088/0741-3335/54/12/124009},
volume = {54},
