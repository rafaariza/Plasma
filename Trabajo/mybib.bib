@article{Vargas_2007,
abstract = {Interpretative transport has been used to revisit the global scalings of TJ-II plasmas with electron cyclotron resonance heating (ECH), from a local perspective. Density, rotational transform and ECH power scans were analysed based mainly upon Thomson scattering data (electron density and temperature) in steady state discharges. The thermal diffusivity $\chi$e is estimated assuming pure diffusion and negligible convective heat fluxes in a set of 201 discharges. The density scan indicates that for normalized minor radii $\rho$ ≲ 0.4 there is no significant change of $\chi$e with density in the range studied 
, while in the density gradient region, $\chi$e decreases with line density. In the rotational transform scan, there is an overall tendency of $\chi$e to decrease with increasing rotational transform, but we also find that the presence of a low order rational value of the rotational transform is accompanied by a local lowering of $\chi$e. Finally, in the ECH power scan, $\chi$e is found to have an overall increment in 0.2 {\textless} $\rho$ {\textless} 0.6 when QECH increases from 200 to 400 kW, although it is less significant in the density gradient region.},
author = {Vargas, V I and L{\'{o}}pez-Bruna, D and Herranz, J and Castej{\'{o}}n, F},
doi = {10.1088/0029-5515/47/9/039},
